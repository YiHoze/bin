{
    "RequirePackage": {
    	"prefix": "req",
		"body": "\\RequirePackage{$0}",
  	},

	"ExplSyntax": {
		"prefix": "expl",
		"body": [
			"\\ExplSyntaxOn",
			"$0",
			"\\ExplSyntaxOff"
		],
	},

	"makeatletter": {
    	"prefix": "makeat",
    	"body": [
			"\\makeatletter", 
			"$0", 
			"\\makeatother"
		],
	  },

	"NewDocumentCommand": {
    	"prefix": "newcom",
    	"body": [
			"\\NewDocumentCommand $0 { }",
			"{" ,
			"\t",
			"}"
		],
	},

	"RenewDocumentCommand": {
    	"prefix": "renewcom",
    	"body": [
			"\\RenewDocumentCommand $0 { }",
			"{" ,
			"\t",
			"}"
		],
	},

  	"DeclareDocumentCommand": {
    	"prefix": "decom",
    	"body": [
			"\\DeclareDocumentCommand $0 { }",
			"{" ,
			"\t",
			"}"
		],
	},

  	"NewDocumentEnvironment": {
    	"prefix": "newenv",
    	"body": [
			"\\NewDocumentEnvironment { $0 } { }",
			"{",
			"\t",
			"}{",
			"\t",
			"}"
		],
	},

	"RenewDocumentEnvironment": {
    	"prefix": "renewenv",
    	"body": [
			"\\RenewDocumentEnvironment { $0 } { }",
			"{",
			"\t",
			"}{",
			"\t",
			"}"
		],
	},

}